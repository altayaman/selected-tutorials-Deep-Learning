	font-size: 0.875rem;
	letter-spacing: 0.05em;
	outline: none;
	padding: 10px 20px;
	text-shadow: none;
	text-transform: uppercase;
	-webkit-transition: color 0.2s, border-color 0.2s;
	-moz-transition: color 0.2s, border-color 0.2s;
	transition: color 0.2s, border-color 0.2s;
}

